{
  "name": "@cib/common-frontend",
  "version": "0.0.5-dev.0",
  "type": "module",
  "description": "Common Vue.js components library for CIB applications",
  "main": "dist/cib-common-frontend.umd.js",
  "module": "dist/cib-common-frontend.es.js",
  "exports": {
    ".": {
      "import": "./dist/cib-common-frontend.es.js",
      "require": "./dist/cib-common-frontend.umd.js"
    },
    "./dist/style.css": "./dist/common-frontend.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "publish:dev": "npm run build && npm version prerelease --preid=dev --force && npm publish --registry https://nexus.cib.de/repository/npm-hosted-dev/ --tag dev"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@popperjs/core": "2.11.8",
    "axios": "1.12.2",
    "bootstrap": "5.3.8",
    "@cib/bootstrap-components": "1.0.0",
    "vue": "3.5.13",
    "vue-i18n": "11.1.10",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "9.26.0",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitest/eslint-plugin": "1.1.44",
    "@vue/test-utils": "2.4.6",
    "cypress": "14.3.3",
    "eslint": "9.39.1",
    "eslint-plugin-cypress": "4.3.0",
    "eslint-plugin-vue": "9.32.0",
    "prettier": "3.5.3",
    "vite": "6.4.1",
    "vitest": "3.1.3"
  },
  "peerDependencies": {
    "axios": "^1.12.2",
    "vue": "^3.5.13"
  }
}
