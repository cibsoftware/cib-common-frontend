{
  "name": "@cib/common-frontend",
  "description": "Common Vue.js components library for CIB applications",
  "version": "1.0.3-dev.0",
  "type": "module",
  "license": "Apache-2.0",
  "main": "dist/common-frontend.umd.js",
  "module": "dist/common-frontend.es.js",
  "style": "dist/common-frontend.css",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/common-frontend.es.js",
      "require": "./dist/common-frontend.umd.js"
    },
    "./dist/style.css": "./dist/common-frontend.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --coverage --reporter=default --reporter=junit --outputFile=target/vitest-reports/TEST-front-end.xml",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "publish:dev": "npm run build && npm version prerelease --preid=dev --force && npm publish --registry https://nexus.cib.de/repository/npm-hosted-dev/ --tag dev"
  },
  "dependencies": {
    "@cib/bootstrap-components": "1.0.3-dev.0",
    "@mdi/font": "7.4.47",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.8",
    "vue": "3.5.16",
    "vue-i18n": "11.1.10",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitest/coverage-istanbul": "3.1.3",
    "@vitest/eslint-plugin": "1.1.44",
    "@vue/test-utils": "2.4.6",
    "cypress": "14.3.3",
    "eslint": "9.39.1",
    "eslint-plugin-cypress": "4.3.0",
    "eslint-plugin-vue": "9.32.0",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "jsdom": "22.1.0",
    "prettier": "3.5.3",
    "vite": "6.4.1",
    "vite-plugin-vue-devtools": "7.7.6",
    "vitest": "3.1.3"
  }
}
